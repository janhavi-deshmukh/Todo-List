<%-include("header")-%>

    <div class="box" id="heading">
        <h1><%=ListTitle%></h1>
    </div>
    
    <div class="box">

         <% for(var i=0;i<newItems.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%=newItems[i]%></p>
            </div>
         <% } %>
               

         <form action="/" method="post" class="item">
            <input type="text" name="inputText" placeholder="New item" autocomplete="off">
            <button type="submit" name="list" value="<%=ListTitle%>">+</button>
        </form> 

    </div>
    
<%- include("footer") -%>